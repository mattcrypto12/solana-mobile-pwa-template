{
  "packageId": "com.example.solanamobilepwa",
  "host": "your-domain.com",
  "name": "Solana Mobile PWA",
  "launcherName": "Solana PWA",
  "display": "standalone",
  "orientation": "portrait",
  "themeColor": "#9945FF",
  "navigationColor": "#0D0D0D",
  "navigationColorDark": "#0D0D0D",
  "navigationDividerColor": "#1A1A1A",
  "navigationDividerColorDark": "#1A1A1A",
