  "isChromeOSOnly": false,
  "isMetaQuest": false,
  "fullScopeUrl": "https://your-domain.com/",
  "minSdkVersion": 23,
  "fingerprints": [],
  "additionalTrustedOrigins": [],
  "retainedBundles": [0],
  "playBilling": {
    "enabled": false
  },
  "alphaDependencies": {
    "enabled": false
  },
  "features": {
    "locationDelegation": {
      "enabled": false
    },
    "playBilling": {
      "enabled": false
    }
  },
  "shareTarget": {
    "enabled": false
  },
  "useBrowserOnChromeOS": false,
  "splashScreenImagePath": "./assets/splash/splash-512x512.png"
